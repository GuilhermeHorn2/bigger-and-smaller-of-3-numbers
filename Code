n1 = float(input(':'))
n2 = float(input(':'))
n3 = float(input(':'))
def minor(n1,n2,n3):
    if n1 < n2 and n1 < n3:
        print(n1)
    elif n2 < n1 and n2 < n3:
        print(n2)
    elif n3 < n1 and n3 < n2:
        print(n3)

def mayor(n1,n2,n3):
    if n1 > n2 and n1 > n3:
        print(n1)
    elif n2 > n1 and n2 > n3:
        print(n2)
    else:
        print(n3)
minor(n1,n2,n3)
mayor(n1,n2,n3)

